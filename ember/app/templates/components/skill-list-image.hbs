{{#if skill.featuredPicture}}
{{#link-to "skill" skill.slug}}<img src="{{skill.featuredPicture.thumbUrl}}" class="skill-list-image">{{/link-to}}
{{else if skill.sport.skillPictureUrl}}
{{#link-to "skill" skill.slug}}<img src="{{skill.sport.skillPictureUrl}}" class="skill-list-image">{{/link-to}}
{{/if}}
